									"IMPORTDECISIONSERVICES": {
										"enabled": "true",
										"description": "Import 'Client Onboarding Decisions' decision service",
										"decisionServiceName": "Client Onboarding Decisions",
										"file": "ClientOnboardingDecisions.zip"
